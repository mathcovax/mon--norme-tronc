{
  "name": "duplojs-project",
  "scripts": {
    "dev": "ENVIRONMENT=DEV tsx --watch src/main --swagger --generate-types",
    "start": "ENVIRONMENT=PROD tsx src/main",
    "lint": "eslint .",
    "test:types": "tsc --noEmit",
    "generate:swagger": "tsx src/main --swagger --only-generate",
    "generate:types": "tsx src/main --generate-types --only-generate"
  },
  "dependencies": {
    "@duplojs/duplojs": "^1.5.16",
    "@duplojs/editor-tools": "^1.2.4",
    "@duplojs/http-exception": "^1.0.1",
    "@duplojs/routes-directory": "^1.2.2",
    "@duplojs/swagger": "^1.0.4",
    "@duplojs/type-input": "^1.0.0",
    "@duplojs/what-was-sent": "^1.2.1",
    "@duplojs/zod-accelerator": "^1.1.5",
    "@types/jsonwebtoken": "^9.0.6",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "firebase-admin": "^12.0.0",
    "i": "^0.3.7",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.4",
    "prisma": "^5.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.3.1"
  }
}
